pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('build') {
            steps {
                script {
                	def buildConfigs = [{"name":"java", "command":"echo 'java'"},{"name":"maven", "command":"echo 'maven'"}]
                	buildConfigs.each{ buildConfig ->
                		sh "echo '$buildConfig.name' && $buildConfig.command"
					}
                }
            }
        }
    }
}